views:
  - type: table
    name: Table
    filters:
      and:
        - file.tags.contains("bill")
    order:
      - file.name
      - due_days
      - amount
      - covered
      - file.tags
    sort:
      - property: due_days
        direction: ASC
    columnSize:
      file.name: 92
      note.amount: 106
